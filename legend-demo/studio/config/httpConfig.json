{
    "pac4j": {
      "callbackPrefix": "/studio/log.in",
      "bypassPaths": ["/studio/admin/healthcheck"],
      "mongoUri": "mongodb://admin:password@legend-demo-mongodb:27017",
      "mongoDb": "legend",
      "clients": [
        {
          "org.finos.legend.server.pac4j.gitlab.GitlabClient": {
            "name" : "gitlab",
            "clientId": "__GITLAB_CLIENT_ID__",
            "secret": "__GITLAB_SECRET__",
            "discoveryUri": "https://gitlab.com/.well-known/openid-configuration",
            "scope": "openid profile api"
          }
        }
      ],
      "mongoSession": {
        "enabled": true,
        "collection": "userSessions"
      }
    },
      "html5Router": true,
      "routerExemptPaths": [
          "/editor.worker.js",
          "/json.worker.js",
          "/editor.worker.js.map",
          "/json.worker.js.map",
          "/version.json",
          "/config.json",
          "/favicon.ico",
          "/static"
      ],
      "localAssetPaths": {
          "/studio/config.json" : "/config/uiConfig.json"
      },
      "uiPath": "/studio",
    "server": {
      "type": "simple",
      "applicationContextPath": "/",
      "adminContextPath": "/studio/admin",
      "connector": {
        "type": "http",
        "port": 8080
      }
    },
    "logging": {
      "level": "INFO",
      "loggers" : {
        "root": { "level": "INFO"},
        "org.pac4j": { "level": "INFO"}
      },
      "appenders": [
        {
          "type": "console"
        }
      ]
    }
  }
  