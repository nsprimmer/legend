{
    "deployment": {
      "mode": "TEST_IGNORE_FUNCTION_MATCH"
    },
    "logging": {
      "level": "info",
      "loggers": {
        "root": {
          "level": "info"
        },
        "org.pac4j": {
          "level": "info"
        }
      },
      "appenders": [
        {
          "type": "console",
          "logFormat": "%d{yyyy-MM-dd HH:mm:ss.SSS} %-5p [%thread] %c - %m%n"
        }
      ]
    },
    "pac4j": {
      "callbackPrefix": "",
      "mongoUri": "mongodb://admin:password@legend-demo-mongodb:27017",
      "mongoDb": "legend",
      "bypassPaths": [
        "/api/server/v1/info"
      ],
      "clients": [
        {
          "org.finos.legend.server.pac4j.gitlab.GitlabClient": {
            "name": "gitlab",
            "clientId": "__GITLAB_CLIENT_ID__",
            "secret": "__GITLAB_SECRET__",
            "discoveryUri": "https://gitlab.com/.well-known/openid-configuration",
            "scope": "openid profile api"
          }
        }
      ],
      "mongoSession": {
        "enabled": true,
        "collection": "userSessions"
      }
    },
    "opentracing": {
      "elastic": "",
      "zipkin": "",
      "uri": "",
      "authenticator": {
        "principal": "",
        "keytab": ""
      }
    },
    "swagger": {
      "title": "Legend Execution REST Services",
      "resourcePackage": "org.finos.legend"
    },
    "server": {
      "type": "simple",
      "applicationContextPath": "/exec",
      "adminContextPath": "/exec/admin",
      "connector": {
        "maxRequestHeaderSize": "32KiB",
        "type": "http",
        "port": 6060
      },
      "requestLog": {
        "appenders": [
        ]
      },
      "rootPath": "/api"
    },
    "metadataserver": {
      "pure": {
        "host": "127.0.0.1",
        "port": 8090
      }
    },
    "vaults": [
      {
        "_type":"property",
        "location":"vault.properties"
      }
    ],
    "relationalexecution": {
      "tempPath": "/tmp/",
      "temporarytestdb": {
        "port": 9092
      }
    }
  }
  
  