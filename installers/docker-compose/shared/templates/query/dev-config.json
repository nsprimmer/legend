{
  "deployment": {
    "mode": "TEST_IGNORE_FUNCTION_MATCH"
  },
  "logging": {
    "level": "info",
    "loggers": {
      "root": {
        "level": "info"
      },
      "org.pac4j": {
        "level": "debug"
      }
    },
    "appenders": [
      {
        "type": "console",
        "logFormat": "%d{yyyy-MM-dd HH:mm:ss.SSS} %-5p [%thread] %c - %m%n"
      }
    ]
  },
  "pac4j": {
    "callbackPrefix": "",
    "bypassPaths": [
      "/api/server/v1/info"
    ],
    "clients": [
      {
        "org.pac4j.core.client.direct.AnonymousClient": {}
      }
    ]
  },
  "opentracing": {
    "elastic": "",
    "zipkin": "",
    "uri": "",
    "authenticator": {
      "principal": "",
      "keytab": ""
    }
  },
  "swagger": {
    "title": "Legend Query",
    "resourcePackage": "org.finos.legend",
    "uriPrefix": "/api"
  },
  "server": {
    "type": "simple",
    "applicationContextPath": "/",
    "adminContextPath": "/admin",
    "connector": {
      "maxRequestHeaderSize": "32KiB",
      "type": "http",
      "port": __LEGEND_QUERY_PORT__
    },
    "requestLog": {
      "appenders": [
      ]
    }
  },
  "metadataserver": {
    "pure": {
      "host": "127.0.0.1",
      "port": 8090
    }
  },
  "vaults": []
}
